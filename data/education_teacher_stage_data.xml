    </record>
    
    <record id="stage_retired" model="education.teacher.stage">
    	<field name="name">Retired</field>
    	<field name="sequence">3</field>
    	<field name="teacher_state">retired</field>
    	<field name="fold">False</field>	<!-- <field name="fold" eval="True"/> -->
    </record>
    
</odoo>
